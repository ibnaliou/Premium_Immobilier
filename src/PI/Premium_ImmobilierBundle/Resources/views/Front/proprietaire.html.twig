<div id="infopersoapro"><!--debut div info perso ancien proritaire-->
    <p class="h4 text-center mb-4">INFORMATIONS PERSONNELLES</p>

    <!-- Material input email -->
    <div class="md-form">
        <i class="fa fa-envelope prefix grey-text"></i>        
        <label for="materialFormLoginEmailEx">Votre Email</label>

        <input type="email" name="mail" id="materialFormLoginEmailEx" class="form-control">
    </div>  

    <!-- Material input password -->
    <div class="md-form">
        <i class="fa fa-lock prefix grey-text"></i>
                <label for="materialFormLoginPasswordEx">Votre Mot de Passe</label>

        <input name="password" type="password" id="materialFormLoginPasswordEx" class="form-control">
    </div>
    
    <a name="" id="next" class="btn btn-dark"  role="button">suivant</a>
</div><!--end div info perso ancien proritaire-->

<div id="infobienapro" ><!--debut div info bien ancien proritaire-->
    <p class="h4 text-center mb-4">INFORMATIONS DU BIEN</p>
    <!-- nom bien -->
    <div class="md-form">
        <i class="fa fa-user prefix grey-text"></i>
                <label for="materialFormLoginPasswordEx">Nom du Bien</label>

        <input name="nombien" type="text"  class="form-control" placeholder="Immeuble R+3" required>
    </div>

    <!-- LOCALITE DU BIEN-->
    <div class="md-form">
        <i class="fa fa-map prefix grey-text"></i>
                <label for="materialFormLoginPasswordEx">Localite du Bien</label>

        <input name="localite" type="text"  class="form-control" placeholder="Pikine" required>
    </div>

    <!-- TYPE DU BIEN-->
    <div class="md-form">
        <i class="fa fa-map prefix grey-text"></i>
                <label for="materialFormLoginPasswordEx">Type du Bien</label>

        <input name="type" type="text" placeholder="Immeuble"   class="form-control" required>
    </div>

    <!-- PRIX DU BIEN-->
    <div class="md-form">
        <i class="fa fa-money prefix grey-text"></i>
                <label for="materialFormLoginPasswordEx">Prix Souhaite</label>

        <input name="prix" type="number" placeholder="20000000"   class="form-control" required>
    </div>

     <!-- IMAGE DU BIEN-->
    <div class="md-form">
        <i class="fa fa-picture prefix grey-text"></i>
                <label for="materialFormLoginPasswordEx"> Images du Bien</label>
        <input name="image1" type="file" placeholder="20000000"   class="form-control" required>
        <input name="image2" type="file" placeholder="20000000"   class="form-control" >
        <input name="image3" type="file" placeholder="20000000"   class="form-control" >
    </div>

    <!--DESCRIPTION DU BIEN-->
    <div class="md-form">
        <i class="fa fa-money prefix grey-text"></i>
                <label for="materialFormLoginPasswordEx">Description du Bien</label>
        <input name="description" type="textarea" placeholder="Un immmeuble situé à la sicap SA supercie de ..."   class="form-control" required>
    </div>

    

    <a name="" id="previous" class="btn btn-dark" role="button">précedent</a>
    <div class="text-center mt-4">
      <input class="btn btn-light" type="submit" name="confier" value="CONFIER">
    </div>
</div><!--end div info bien ancien proritaire-->
    
</form>


